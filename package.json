    "lint:dependencies": "npx ember dependency-lint",
    "release": "release-it",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
